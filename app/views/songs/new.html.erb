<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title%>

  <%= f.collection_select(:artist_id, Artist.all.order(:name), :id, :name ) %>


  <%= f.label :num %>
  <%= f.text_field :num%>

  <%= f.collection_select(:genre_id, Genre.all.order(:name), :id, :name ) %>

  <%= f.collection_select(:vibe_ids, Vibe.all.order(:name), :id, :name ) %>


  <%= f.submit "Add Song"%>
<% end %>
